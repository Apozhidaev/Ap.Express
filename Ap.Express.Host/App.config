<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="apExpress" type="Ap.Express.ContentSection, Ap.Express, Version=1.0.0.0, Culture=neutral" />
    <section name="network" type="Ap.Express.Host.Configuration.NetworkSection, Ap.Express.Host, Version=1.0.0.0, Culture=neutral" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <network url="http://localhost:3838/"
           serviceName="Ap.Express.Host"
           displayName="Ap.Express.Host"
           description="Ap.Express.Host"
           root="..\..\wwwroot\"/>
  <apExpress defaultMediaType="application/octet-stream" defaultUrl="index.html">
    <mediaTypes>
      <mediaType extension=".html" value="text/html"/>
      <mediaType extension=".js" value="application/javascript"/>
      <mediaType extension=".css" value="text/css"/>
      <mediaType extension=".ico" value="image/vnd.microsoft.icon"/>
      <mediaType extension=".png" value="image/png"/>
      <mediaType extension=".jpg" value="image/jpeg"/>
      <mediaType extension=".eot" value="application/vnd.ms-fontobject"/>
      <mediaType extension=".svg" value="image/svg+xml"/>
      <mediaType extension=".ttf" value="application/font-ttf"/>
      <mediaType extension=".woff" value="application/font-woff"/>
      <mediaType extension=".woff2" value="application/font-woff"/>
      <mediaType extension=".appcache" value="text/cache-manifest"/>
    </mediaTypes>
  </apExpress>
</configuration>